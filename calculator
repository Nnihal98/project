print('Enter two number')
n1 =int(input('number'))
n2 =int(input('number'))
print('What you want')
print('type 1 for +')
print('type 2 for -')
print('type 3 for *')
print('type 4 for /')

n3 =int(input('number'))

if(n3==1):
 x=n1+n2
 print(x)
elif(n3==2):
 x=n1-n2
 print(x)
elif(n3==3):
 x=n1*n2
 print(x)
elif(n3==4):
 x=n1/n2
 print(x)


